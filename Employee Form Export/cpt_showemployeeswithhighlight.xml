entry OccurrenceSetFieldColors
	params
		string    pEntity : IN
		string    pColor  : IN
	endparams
	variables
		string    vField, vColor
	endvariables
	
	; Get the name of a painted field
	getitem vField, $entinfo(pEntity, "PAINTEDFIELDS"), 1
	
	; Set the fields to the requested color
	forlist vField in $entinfo(pEntity, "PAINTEDFIELDS")
		$fieldproperties(vField, "backcolor") = "backcolor=%%pColor%%%"
	endfor
end

entry handleSort
	params
		string fieldName : IN
	endparams

	variables
		string FsortOrder
		numeric vloops
	endvariables
	
	; sort occurrences based on the clicked column
	if ($sortOrder$ = "%%fieldname%%%:a")
		$sortOrder$ = "%%fieldname%%%:d"
	else
		$sortOrder$ = "%%fieldname%%%:a"
	endif
	
	sort/e "&lt;$entname&gt;", $sortOrder$
	setocc "&lt;$entname&gt;",1
	
	vloops = 0
	forentity "&lt;$entname&gt;"
		vloops += 1
		call OccurrenceSetFieldColors( "&lt;GRIDENTITY&gt;", "#FFFFFF")    
		if (vloops %2)
			call OccurrenceSetFieldColors( "&lt;GRIDENTITY&gt;", "&lt;SELECTIONCOLOR&gt;")
		endif
	endfor
	setocc "&lt;$entname&gt;",1
	$prompt = fieldName
end

