  ; Your implementation here, e.g.:
  retrieve
  if ($status &lt; 0)
    message $text(1762)
  endif

  return $status

end


trigger store
; This trigger is fired on macro ^STORE(GOLD-S) and typically performs a data store

  ; Your implementation here, e.g.:
  store
  if ($status &lt; 0)
    message $text(1500)    ; error
    rollback
  else
    if ($status = 1)
      message $text(1723)   ; no change
    else
      commit
      if ($status &lt; 0)
        rollback
      else
        message $text(1805) ; Ok
      endif
    endif
  endif

  return $status

end


trigger erase
; This trigger is fired on macro ^ERASE (GOLD-E) and typically erases retrieved data

  ; Your implementation here, e.g.:
  erase
  if ($status &lt; 0)
    message $text(1763)   ;error
    rollback
  else
    if ($status = 1)
      message $text(1634) ;not allowed
    else
      message $text(1806) ;Ok
      commit
    endif
  endif

  return $status

end</DAT>
